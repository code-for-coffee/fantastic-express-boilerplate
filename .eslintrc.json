{
  "plugins": [
    "react"
  ],
  "env": {
    "es6": true,
    "shared-node-browser": true
  },
  "globals":{
    "require": true,
    "process": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "no-set-state": "off",
    "no-console": ["off"],
    "no-undef": ["off"],
    "no-unused-vars": ["off"]
  }
}